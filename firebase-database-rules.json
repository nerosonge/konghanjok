{
  "rules": {
    "waiting": {
      "$queueKey": {
        "$uid": {
          ".read": "auth != null && (auth.uid == $uid)",
          ".write": "auth != null && auth.uid == $uid"
        }
      }
    },
    "rooms": {
      "$roomId": {
        ".read": "auth != null",
        ".write": "auth != null"
      }
    }
  }
}
